 <%= form_for @project do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name, :autofocus => true %><br />

  <%= f.label :description %>
  <%= f.text_area :description %><br />

  <%= f.label :manager_id %>
  <%= f.text_field :manager_id %><br />

  <%= f.label :region_id %>
  <%= f.text_field :region_id %><br />

  <%= f.label :need_level %>
  <%= f.text_field :need_level %><br />

  <%= f.label :need_list %>
  <%= f.text_field :need_list %><br />

  <%= f.submit %>
<% end %>
